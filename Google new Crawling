        "def is_prime(m):\n",
        "    for i in range(2,m):\n",
        "        if m % i== 0:\n",
        "            return False\n",
        "    return True\n",
        " \n",
        "m = eval(input(''))\n",
        "for i in range(2,m+1):\n",
        "    i_is_prime = is_prime(i)\n",
        "    if i_is_prime:\n",
        "        print(i,'is prime')"
      ]
    },
