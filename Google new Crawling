        "#3068. dict字典練習II \n",
        "keys=['P','M','H']\n",
        "value=['Pikachu','Mickey Mouse','Hello kitty']\n",
        "d=dict(zip(keys,value)) #dict(zip())功能將keys and value連結字典\n",
        " \n",
        "while True:\n",
        "  keyin=input()\n",
        "  if keyin == '-1':\n",
        "    break\n",
        "  elif keyin in d: #d=被連結之字典\n",
        "    print(d[keyin])\n",
        "  elif keyin =='-2':\n",
        "    for i in len:\n",
        "      value.append(i)\n",
        "      keys.append(i)\n",
        "    print(\"keys:\",keys)\n",
        "    print(\"values:\",value)\n",
        "  else:\n",
        "    print('%s'%keyin,'does not exist. Enter a new one:')\n",
        "    new_data=input()\n",
        "    d[keyin]=new_data\n",
        "\n",
        "\n",
        "\n",
        "\n"
      ]
