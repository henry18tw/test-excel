      "source": [
        "import numpy as np\n",
        "sum=0\n",
        "avg=0\n",
        "\n",
        "def avg(st):\n",
        "    ans = np.sum(st)/len(st)\n",
        "    #計算平均\n",
        "    return ans\n",
        " \n",
        "lst = [[76,73,85],[88,84,76],[92,82,92],[82,91,85],[72,74,73]]\n",
        "for i in range(len(lst)):\n",
        "    print('student', i+1)\n",
        "    #print(lst[i])\n",
        "    for j in range(len(lst[i])):\n",
        "        print(\" %d: %d\"%(j+1,lst[i][j]))\n",
        "    print(\" sum: %d\"%(np.sum(lst[i])))\n",
        "    print(\" avg: %.2f\"%(avg(lst[i])))\n",
        "\n",
        "print(\"total: %d,\"%(np.sum(lst)),\" avg: %.2f\"%(avg(lst)/3),sep='')\n",
        "print(\"highest avg: student\",'%.d:'%(lst.index(max(lst))+1),\"%.2f\"%avg(max(lst)))"
      ]
    },
