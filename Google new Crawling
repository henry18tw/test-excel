        "#M,N=代表頭的數量和脚的數量\n",
        "M,N=map(int,input().split())\n",
        "\n",
        "c=0\n",
        "#r=0\n",
        "#M=c+r\n",
        "#N=2*c+4*(M-c)\n",
        "\n",
        "while ((2*c)+(4*(M-c)))!=N:\n",
        "  c+=1\n",
        "\n",
        "  if (((2*c)+(4*(M-c)))%N)==0:\n",
        "    print('YES')\n",
        "    print(c,M-c)\n",
        "  else:\n",
        "    print('NO')\n",
        "\n",
        "\n",
        "\n"
