        "class student:\n",
        "  def __init__(self,name,gender):\n",
        "    self.name = name\n",
        "    self.gender = gender\n",
        "    self.grades =[]\n",
        "  def avg(self):\n",
        "    return sum(self.grades)/len(self.grades)\n",
        "  def add(self,grade):\n",
        "    self.grades.append(grade)\n",
        "  def fcount(self):\n",
        "    fct = 0\n",
        "    for i in range(len(self.grades)):\n",
        "      if self.grades[i] < 60:\n",
        "        fct+=1\n",
        "      else:\n",
        "        fct+=0\n",
        "    return fct\n",
        "  def show(self):\n",
        "    print(\"Name: %s\"%self.name,\"Gender: %s\"%self.gender,\"Graders: %s\"%self.grades,'Avg: %.2f'%(self.avg()),\"Fail number: %s\"%self.fcount(),sep=\"\\n\")\n",
        "    #sum(self.grades)\n",
        "  \n",
        "  def top(self):\n",
        "    max_avg = [k for k, x in avg() if not any(y > x for y in avg())]\n",
        "    print('成绩最高的是:')\n",
        "    for student in max_avg:\n",
        "      print(student)\n",
        "  def top(self,grades):\n",
        "    top_student = max(s1.avg(),s2.avg(),s3.avg(),s4.avg(),s5.avg())\n",
        "    return top_student\n",
        "\n",
        "    \n",
        "s1 = student(\"Tom\",\"M\")\n",
        "s2 = student(\"Jane\",\"F\")\n",
        "s3 = student(\"John\",\"M\")\n",
        "s4 = student(\"Ann\",\"F\")\n",
        "s5 = student(\"Peter\",\"M\")\n",
