      "source": [
        "import math\n",
        "A,B,C =input().split()\n",
        "l=float(A)\n",
        "w=float(B)\n",
        "r=float(C)\n",
        "\n",
        "pi=3.1415926\n",
        "\n",
        "area=pi*r*r*(270/360)\n",
        "darea=float(area)\n",
        "\n",
        "if w>=r and l>=r:\n",
        "  print('%.6f'%darea)\n",
        "elif (w>=r and l<=r):\n",
        "  print('%.6f'%(darea+((r-l)**2*pi*(90/360))))\n",
        "elif (w<=r and l>=r):\n",
        "  print('%.6f'%(darea+((r-w)**2*pi*(90/360))))\n",
        "elif (w<=r and l<=r) and ((w+l)>=r) :\n",
        "  print(('%.6f'%(darea+(((r-w)**2*pi*(90/360))+(r-l)**2*pi*(90/360)))))\n",
        "else:\n",
        "  print('')"
      ]
    },
