        "class student:\n",
        "  def __init__(self,name,gender):\n",
        "    self.name = name\n",
        "    self.gender = gender\n",
        "    self.grades =[]\n",
        "  def avg(self):\n",
        "    return sum(self.grades)/len(self.grades)\n",
        "  def add(self,grade):\n",
        "    self.grades.append(grade)\n",
        "  def fcount(self):\n",
        "    fct = 0\n",
        "    for i in range(len(self.grades)):\n",
        "      if self.grades[i] < 60:\n",
        "        fct+=1\n",
        "      else:\n",
        "        fct+=0\n",
        "    return fct\n",
        "  def show(self):\n",
        "    print(self.name,self.gender,sum(self.grades),self.avg(),self.fcount())\n",
        "\n",
        "s1 = student(\"Tom\",\"M\")\n",
        "s2 = student(\"Jane\",\"F\")\n",
        "s3 = student(\"John\",\"M\")\n",
        "s4 = student(\"Ann\",\"F\")\n",
