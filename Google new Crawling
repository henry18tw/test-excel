        "if a>b and a>c:\n",
        "  print('True')\n",
        "else:\n",
        "  print('False')\n",
        "\n",
        "if a<b and a<c:\n",
        "  print('True')\n",
        "else:\n",
        "  print('False')\n",
        "\n",
        "if (a<b or a<c):\n",
        "  print('True')\n",
        "else:\n",
        "  print('False')\n",
        "\n",
        "if (a>b or a>c):\n",
        "  print('True')\n",
        "else:\n",
        "  print('False')\n"
      ]
    },
