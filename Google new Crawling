      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "from sklearn import preprocessing\n",
        "\n",
        "Input_data = np.array([[2.1,-1.9,2.5],\n",
        "            [-1.5,2.4,3.5],\n",
        "            [0.5,-7.9,5.6],\n",
        "            [5.9,2.3,-5.8]])\n",
        "data_binarized = preprocessing.Binarizer(threshold=0.5).transform(Input_data)  #將>0.5之數值轉換為1，<=0.5之數值轉換為0\n",
        "print(\"\\bBinarized data:\\n\",data_binarized)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
