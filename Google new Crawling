      "source": [
        "#1406 阿宅的學習影片\n",
        "A,B =input().split()\n",
        "L=int(A)\n",
        "S=int(B)\n",
        "\n",
        "cnt=0\n",
        "\n",
        "while L>S:\n",
        "  S+=5\n",
        "  cnt+=1\n",
        "  while L<S:\n",
        "    S-=2\n",
        "    cnt+=1\n",
        "\n",
        "while L<S:\n",
        "  S-=2\n",
        "  cnt+=1\n",
        "  while L>S:\n",
        "    S+=5\n",
        "    cnt+=1\n",
        "\n",
        "print(cnt,end='\\n')"
      ]
    },
