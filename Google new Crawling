        "#擷取doodles網址\n",
        "from urllib.request import urlopen\n",
        "response = urlopen(\"https://www.google.com/doodles/json/2019/11?hl=zh_TW\")\n",
        "doodles = json.load(response)\n",
        "#print(response)\n",
        "#print(doodles)\n",
        "for d in doodles:\n",
        "  title = d[\"title\"]\n",
        "  url = \"https:\"+d[\"url\"]\n",
        "  alternate_url = d[\"alternate_url\"]\n",
        "  print(\"圖片標題\", title)\n",
        "  print(\"圖片網址:\", url)\n",
        "  print(\"替代網址:\", alternate_url)"
      ]
