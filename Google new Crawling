      "source": [
        "total=0\n",
        "i=0\n",
        "while True:\n",
        "  x=eval(input())\n",
        "  if x==-1:\n",
        "    break\n",
        "  else:\n",
        "    total=total+x\n",
        "    i=i+1\n",
        "    avg=(total)/i\n",
        "print(total)\n",
        "print('%.1f'%avg)\n"
      ]
    },
