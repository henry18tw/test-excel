        "#3073. 函式回傳多個數值\n",
        "\n",
        "def return2num(n=0):\n",
        "  \n",
        "  if n==0:\n",
        "    sum_a=0\n",
        "    fact_a=1\n",
        "    return fact_a, sum_a\n",
        "  else:\n",
        "    sum=0\n",
        "    sumf=1\n",
        "    for i in range(1,n+1):\n",
        "      sum+=i\n",
        "      sumf*=i\n",
        "    return sumf,sum\n",
        "\n",
        "x=eval(input())\n",
        "t=list(return2num(x))\n",
        "print(t[0])\n",
        "print(t[1])\n"
      ]
