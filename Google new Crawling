        "import math\n",
        "\n",
        "a=str(int(input()))\n",
        "b=str(int(input()))\n",
        "c=float(input())\n",
        "d=str(math.floor(c))\n",
        "a_1=a.zfill(7)\n",
        "b_1=b.zfill(7)\n",
        "d_1=d.zfill(7)\n",
        "\n",
        "for i in range(-4,0):\n",
        "  t=int(a_1[i])+int(b_1[i])+int(d_1[i])\n",
        "  print(t,'%.2f'%(t/3),'%.2f'%((((int(a_1[i])-(t/3))**2)+((int(b_1[i])-(t/3))**2)+((int(d_1[i])-(t/3))**2))/3)**0.5)\n",
        "\n",
        "sum=int(a)+int(b)+c\n",
        "print('%.2f'%(sum),'%.2f'%((sum)/3),'%.2f'%((((int(a)-(sum/3))**2)+((int(b)-(sum/3))**2)+((c-(sum/3))**2))/3)**0.5)\n"
      ]
