        "r=0\n",
        "c=0\n",
        "def Cr(a,b): #a頭數、b腳數、c雞、r兔\n",
        "  r=0\n",
        "  c=0\n",
        "  while True:\n",
        "   \n",
        "    if a!= 0 and ((b-4*r)/(b* 2 ))==1:\n",
        "      a=r+c\n",
        "      b=4*r+2*c \n",
        "      for i in range(a):\n",
        "        r+=1\n",
        "    elif a<0:\n",
        "      print('NO')\n",
        "    else:\n",
        "      print('NO')\n",
        "    return (c,r)\n",
        "\n",
        "m= int ( input ( \"請輸入雞和兔的總數:\" ))\n",
        "n= int ( input ( \"請輸入雞和兔的腳數:\" ))\n",
