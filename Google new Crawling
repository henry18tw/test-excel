        "#1401. 輸入資料&判斷式\n",
        "from ast import literal_eval\n",
        "sum_int=0\n",
        "sum_float=0\n",
        "t=[]\n",
        "\n",
        "x=input()\n",
        "while True:\n",
        "  if x!='q':\n",
        "    x=input()\n",
        "    t.append(x)\n",
        "  else:\n",
        "    break\n",
        "\n",
        "for i in range(0,len(t)):\n",
        "  if isinstance(ast.literal_eval(t[i]),int)==True:\n",
        "    sum_int+=ast.literal_eval(t[i])\n",
        "  elif isinstance(ast.literal_eval(t[i]),float)==True:\n",
        "    sum_float+=ast.literal_eval(t[i])\n",
        "  else:\n",
        "    sum_int=sum_int\n",
        "    sum_float=sum_float\n",
        "\n",
        "print(sum_int)\n",
        "print(sum_float)"
