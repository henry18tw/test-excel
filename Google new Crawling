        "#1412. 身分證檢驗 II\n",
        "\n",
        "dict= {'A':10,'J':18,'S':26,\n",
        "'B':11,'K':19,'T':27,\n",
        "'C':12,'L':20,'U':28,\n",
        "'D':13,'M':21,'V':29,\n",
