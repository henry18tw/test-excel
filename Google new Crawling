        "from urllib.request import urlopen\n",
        "\n",
        "# 使用內建的json 函式庫\n",
        "import json\n",
        "response = urlopen(\"https://www.google.com/doodles/json/2019/12?hl=zh_TW\")\n",
        "doodles = json.load(response)\n",
        "print(doodles)\n",
        "\n",
        "for d in doodles:\n",
        "# 補上https, 使用[名字] 來取得對應的資料\n",
        "  url = \"https:\" + d[\"url\"]\n",
        "  title = d[\"title\"]\n",
        "  print(\"圖片標題:\", title)\n",
        "  print(\"圖片網址:\", url)"
      ]
