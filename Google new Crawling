      ],
      "source": [
        "class student:\n",
        "  def __init__(self,name,gender):\n",
        "    self.name = name\n",
        "    self.gender = gender\n",
        "    self.grades =[]\n",
        "  def avg(self):\n",
        "    return sum(self.grades)/len(self.grades)\n",
        "  def add(self,grade):\n",
        "    self.grades.append(grade)\n",
        "  def fcount(self):\n",
        "    fct = 0\n",
        "    for i in range(len(self.grades)):\n",
        "      if self.grades[i] < 60:\n",
        "        fct+=1\n",
        "      else:\n",
        "        fct+=0\n",
        "    return fct\n",
        "  def show(self):\n",
        "    print(\"Name: %s\"%self.name,\"Gender: %s\"%self.gender,\"Grades: %s\"%self.grades,'Avg: %.1f'%(self.avg()),\"Fail Number: %s\"%self.fcount(),sep=\"\\n\")\n",
        "    #sum(self.grades)\n",
        "  \n",
        "def top(s1,s2,s3,s4,s5):\n",
        "  studentlist = [s1,s2,s3,s4,s5]\n",
        "  avg_max = [s1.avg(),s2.avg(),s3.avg(),s4.avg(),s5.avg()]\n",
        "  address = avg_max.index(max(avg_max))\n",
        "  avgH = studentlist[address]\n",
        "  return avgH\n",
        "\n",
        "\n",
        "    \n",
        "s1 = student(\"Tom\",\"M\")\n",
        "s2 = student(\"Jane\",\"F\")\n",
        "s3 = student(\"John\",\"M\")\n",
        "s4 = student(\"Ann\",\"F\")\n",
        "s5 = student(\"Peter\",\"M\")\n",
        "s1.add(80)\n",
        "s1.add(90)\n",
        "s1.add(55)\n",
        "s1.add(77)\n",
        "s1.add(40)\n",
        "s2.add(58)\n",
        "s2.add(87)\n",
        "s3.add(100)\n",
        "s3.add(80)\n",
        "s4.add(40)\n",
        "s4.add(55)\n",
        "s5.add(60)\n",
        "s5.add(60)\n",
        " \n",
        "s1.show()\n",
        "print(\"\")\n",
        "s2.show()\n",
        "print(\"\")\n",
        "s3.show()\n",
        "print(\"\")\n",
        "s4.show()\n",
        "print(\"\")\n",
        "s5.show()\n",
        "print(\"\")\n",
        "print(\"Top Student:\")\n",
        "top(s1,s2,s3,s4,s5).show()\n",
        "print(\"\")"
      ]
    },
